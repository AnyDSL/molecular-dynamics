fn accelerator_allocate(size: i32) -> Buffer {
    let acc = accelerator(device_id);
    acc.alloc(size)
}

fn transfer_to_accelerator(source: Buffer, destination: Buffer, size: i32) -> () {
    copy(source, buffer, size);
}
