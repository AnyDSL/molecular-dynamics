fn init_constants(constants: &mut Constants) -> () {
    constants.r_cut = 2.5;
    constants.sigma = 1.0;
    constants.epsilon = 5.0;
    //constants.epsilon = 1.0;
    let mut tmp = constants.sigma * constants.sigma * constants.sigma;
    tmp = tmp * tmp;
    constants.tmp1 = 24.0 * constants.epsilon * tmp;
    constants.tmp2 = 1.0/(2.0*tmp);
}


