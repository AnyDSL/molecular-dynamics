fn init_constants(constants: &mut Constants) -> () {
    constants.r_cut = 2.5;
    constants.sigma = 1.0;
    constants.epsilon = 5.0;
    constants.r_cut_sqr = constants.r_cut*constants.r_cut;
    let sigma3 = constants.sigma * constants.sigma * constants.sigma;
    let sigma6 = sigma3 * sigma3;
    constants.tmp1 = 24.0 * constants.epsilon * sigma6;
    constants.tmp2 = 2.0 * sigma6;
}


